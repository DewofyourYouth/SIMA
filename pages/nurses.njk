{% set page_name = 'Nurses' %}
{% extends 'page-layout.njk' %}
{% block pageContent %}

<!-- Nurses -->
    <section id="nurses">
        <div class="row">
        <div class="col-12">
        <div id="doc-pics">
                {% for person in doctor_pics %}
                        <div class="doc-pic">
                           
                                
                            
                            <a href="#">
                                <img src="{{person.src}}" alt="">
                            </a>
                            <a href="#">
                                <div class="nurse-info">
                                    <p class="p-name">{{person.name}}</p>
                                    <p class="p-title">{{person.title}}</p>
                                </div>
                            </a>
                            
                        </div>
                    
                {% endfor %}</div>
{% for person in people_pics %}
 <img src="{{person.src}}" alt="">
{% endfor %}
{% endblock pageContent %}
  

  
