{% set page_name = 'Nurses' %}
{% extends 'page-layout.njk' %}
{% block pageContent %}

    <!-- Nurses -->
    <section id="nurses">
        <div class="row">
            <div class="col-12">
                <div id="doc-pics">
                    {% for person in people_pics %}
                        <div class="doc-pic">

                            <a href="{{person.target}}">
                                <img src="{{person.src}}" alt="">
                            </a>
                            <a href="{{person.target}}">
                                <div class="nurse-info">
                                    <p class="p-name">{{person.name}} {{person.target}}</p>
                                    <p class="p-title">{{person.title}}</p>
                                </div>
                            </a>

                        </div>

                    {% endfor %}
                </div>
            {% endblock pageContent %}